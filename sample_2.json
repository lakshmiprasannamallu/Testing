{
  "type": "script",
  "seleniumVersion": "2",
  "formatVersion": 2,
  "steps": [
    {
      "type": "get",
      "url": "http://www.position2.com/"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "link text",
        "value": "SOLUTIONS"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "css selector",
        "value": "button.dl-trigger"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "css selector",
        "value": "button.dl-trigger.dl-active"
      }
    },
    {
      "type": "verifyTextPresent",
      "text": "Best of both SEO and SMM with existing assets Improves visibility of the site on search engines and social media platforms Increases relevant traffic to website resulting in increased time spent on site"
    },
    {
      "type": "verifyElementPresent",
      "locator": {
        "type": "css selector",
        "value": "img.img-responsive"
      }
    }
  ],
  "data": {
    "configs": {},
    "source": "none"
  },
  "inputs": [],
  "timeoutSeconds": 60
}